  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "hotjar"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavloko/gatsby-plugin-hotjar.git"
  },
  "author": "Pavlo Kochubei <pavlo.kochubei@hotmail.com> (https://pavlokochubei.com/)",
